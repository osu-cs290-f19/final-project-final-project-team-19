<div class='groupEntry'>
	<div class='groupEntry-header'>
		<img class='arrowIcon' src='./keyboard_arrow_right-24px.svg' alt='Group arrow' />
		<h3 style='margin: 0px; padding: 5px;' onclick="toggleEntry(this.parentElement.parentElement)">{{name}}</h3>
	</div>
	<div class='entryBody'>
		<p>{{description}}</p>
		<button style='margin-left: auto; height: 100%;'>Join</button>

		<ul>
			{{#each members}}
			<li>{{this}}</li>
			{{/each}}
		</ul>

	</div>
</div>